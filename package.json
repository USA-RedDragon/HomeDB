{
  "name": "homedb",
  "version": "1.0.0",
  "description": "Home Managment Software",
  "scripts": {
    "start": "node server.js",
    "build": "NODE_ENV=production webpack --config client/config/webpack.config.prod.js",
    "dev": "npm-run-all --parallel server client",
    "server": "nodemon server.js",
    "client": "NODE_ENV=development webpack --config client/config/webpack.config.dev.js --watch"
  },
  "author": {
    "name": "Jacob McSwain",
    "email": "jacob.a.mcswain@gmail.com",
    "url": "https://mcswainsoftware.com/"
  },
  "license": "GPL-2.0-only",
  "repository": {
    "type": "git",
    "url": "git://github.com/USA-RedDragon/homedb.git"
  },
  "dependencies": {
    "bcrypt": "^3.0.0",
    "hapi": "^17.5.4",
    "hapi-auth-jwt2": "^8.1.0",
    "hapi-pino": "^5.1.0",
    "hapi-sequelizejs": "^1.2.0",
    "inert": "^5.1.0",
    "joi": "^13.6.0",
    "jsonwebtoken": "^8.3.0",
    "mysql2": "^1.6.1",
    "nodemon": "^1.18.4",
    "npm-run-all": "^4.1.3",
    "path": "^0.12.7",
    "react": "^16.5.0",
    "react-bootstrap": "^0.32.4",
    "react-dom": "^16.5.0",
    "react-router-dom": "^4.3.1",
    "react-scripts": "1.1.5",
    "sequelize": "^4.38.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "client/*"
    ],
    "delay": "2500"
  }
}
